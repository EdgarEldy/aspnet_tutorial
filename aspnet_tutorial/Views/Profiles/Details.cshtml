@model aspnet_tutorial.Models.profile

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Templates/Default/default_layout.cshtml";
}

<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">Details</div>
        <div class="panel-body">
            <table>
                <tbody>
                <tr>
                    <td>
                        Profile name :
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.profile_name)
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
